<%= provide(:title, 'Partners') %>
<div class="whole-wrap">
  <div class="container box_1170">
    <% @partners.each do |partner| %>
    <div class="section-top-border">
      <h3 class="mb-30"><%= partner.name %></h3>
      <div class="row">
        <div class="col-md-3">
          <img src="<%= url_for(partner.image) %>" alt="" class="img-fluid">
        </div>
        <div class="col-md-9 mt-sm-20">
          <p><%= partner.content %></p>
          <a href="http://<%= partner.website %>" target="_blank">
            <p style="color: #c30147;">Visit website</p>
          </a>
        </div>
      </div>
    </div>
    <% end %>
  </div>
</div>